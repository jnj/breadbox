<h1>New stock</h1>

<% form_for(@stock) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :symbol %><br />
    <%= f.text_field :symbol %>
  </p>
  <p>
    <%= f.label :num_shares %><br />
    <%= f.text_field :num_shares %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<%= hidden_field_tag :account_id, @account.id %>
<% end %>

<%= link_to 'Back', stocks_path %>
